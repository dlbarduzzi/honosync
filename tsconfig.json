{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "module": "NodeNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "node"
    ],
    "strict": true,
    "noImplicitAny": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
